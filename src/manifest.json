{
  "name": "miro-helper",
  "description": "miro chrome extention helper",
  "version": 0.1,
  "manifest_version": 2,
  "icons": {
    "48": "icons/dig-dug-48.png",
    "128": "icons/dig-dug-128.png"
  },
  "browser_action": {
    "default_title": "miro-helper",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
